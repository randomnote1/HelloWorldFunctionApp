{
    "version": "2.0",
    "logging": {
      "applicationInsights": {
        "enableDependencyTracking": true,
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        }
      },
      "console": {
        "isEnabled": true,
        "DisableColors": false
      },
      "fileLoggingMode": "always",
      "logLevel": {
        "default": "Warning",
        "Function.Get-PublicIPAddress": "Trace"
      }
    },
    "extensionBundle": {
      "id": "Microsoft.Azure.Functions.ExtensionBundle",
      "version": "[3.*, 4.0.0)"
    },
    "managedDependency": {
      "enabled": true
    }
  }